{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "dependsOn": ["build"],
      "cache": true
    },
    "test:coverage": {
      "dependsOn": ["build"],
      "cache": true
    },
    "typecheck": {
      "dependsOn": ["^build"]
    }
  }
}
